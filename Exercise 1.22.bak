#reader(lib "htdp-beginner-reader.ss" "lang")((modname |Exercise 1.22|) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(define (timed-prime-test n)
  (newline)
  (display n)
  (start-prime-test n (runtime)))

(define (start-prime-test n start-time)
  (if (prime? n)
      (report-prime (- (runtime) 
                       start-time))))

(define (report-prime elapsed-time)
  (display " *** ")
  (display elapsed-time))

(define (search-for-primes start end)
  (if (= start end)
      (display 'QED)
      (else
       (timed-prime-test n)
       (search-for-primes (+ start 1) end))))






  